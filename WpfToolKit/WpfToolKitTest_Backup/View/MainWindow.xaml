<Window x:Class="WpfToolKitTest.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfToolKitTest"
        xmlns:view="clr-namespace:WpfToolKitTest.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="700" Width="1100" 
        Style="{StaticResource Style.App.Window.Main}"
        WindowStartupLocation="CenterScreen"
        Loaded="Window_Loaded"
        MouseMove="Window_MouseMove">
    <Window.Resources>
    </Window.Resources>
    <Grid>
        <Frame Name="MainPage" NavigationUIVisibility="Hidden" ScrollViewer.CanContentScroll="False" VerticalAlignment="Stretch" VerticalContentAlignment="top" Margin="0 20 0 0"/>
    </Grid>
</Window>
